# API key used for sending POST requests to the Pangram proxy server.
REACT_APP_PANGRAM_API_KEY=your_api_key_here

# Base URL for your backend API (Firebase Cloud Functions)
# Used to prefix all /api/... requests in production.
# For local development uncomment the top, for prod the bottom.

# Local development
REACT_APP_API_BASE_URL=your_local_express_api_base_url_here

# Production
# REACT_APP_API_BASE_URL=your_prod_express_api_base_url_here

# Firebase configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id
REACT_APP_FIREBASE_FIRESTORE_COLLECTION=your_firestore_collection_name