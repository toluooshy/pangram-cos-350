# Name of the session cookie used by Express for logged-in users
# Keep this consistent if you deploy multiple instances behind a load balancer
SESSION_NAME=session

# Secret key used to encrypt/sign session cookies
# MUST be kept private; change this to a strong random string in production
SESSION_SECRET=super-secret-key

# Base URL for Princeton's CAS server
# Used for login, ticket validation, and logout
CAS_URL=https://fed.princeton.edu/cas/

# URL of your frontend React app
# Used as the service URL for CAS redirects and logout redirects
CLIENT_URL=http://localhost:3000

# Firestore collection name where user usage counts are stored
# Each document is keyed by user email
FIRESTORE_COLLECTION=usage

# Maximum number of allowed uses per user
# This is enforced in the backend before incrementing usage
MAX_USAGE=5
